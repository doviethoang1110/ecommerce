<!doctype html>
<html lang="en">
<head>
    <%- include('layout/css');%>
</head>
<body>
<%- include('layout/header'); %>
<%- include('layout/breadcumb');%>
<section class="pwd-page section-b-space">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 offset-lg-3">
                <% if(result) { %>
                    <% if(result.error) { %>
                        <h2 class="text-danger"><%=result.error%></h2>
                    <% }else { %>
                        <h2 class="text-success"><%=result.success%></h2>
                        <a href="/dang-nhap" class="btn btn-solid">Đăng nhập ngay</a>
                    <% } %>
                <% } %>
            </div>
        </div>
    </div>
</section>
<%- include('layout/footer'); %>
</body>
</html>